set logscale y
set title "Residuals"
set ylabel "Residuals"
set xlabel "nยบ of iteration (or time [sec])"

set grid
set key top left autotitle columnhead
set datafile commentschar '#'

plot 	"tables/solverInfo_plot.dat" using "Time":"Ux_initial" title 'Ux' with lines,\
		"tables/solverInfo_plot.dat" using "Time":"Uy_initial" title 'Uy' with lines,\
		"tables/solverInfo_plot.dat" using "Time":"Uz_initial" title 'Uz' with lines,\
		"tables/solverInfo_plot.dat" using "Time":"rho_initial" title 'rho' with lines,\
		"tables/solverInfo_plot.dat" using "Time":"e_initial" title 'e' with lines
pause 1
reread

pause 1
reread
