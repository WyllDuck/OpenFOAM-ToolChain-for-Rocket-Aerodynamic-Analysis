set title "Force Coefficients"
set ylabel "Force Coefficients"
set xlabel "nยบ of iteration (or time [sec])"

set grid
set key top left autotitle columnhead
set datafile commentschar '#'

plot 	"tables/forceCoeffs_plot.dat" using "Time":"Cd" title 'Cd' with lines,\
 		"tables/forceCoeffs_plot.dat" using "Time":"CmPitch" title 'CmPitch' with lines,\
		"tables/forceCoeffs_plot.dat" using "Time":"Cl" title 'Cl' with lines
pause 1
reread

