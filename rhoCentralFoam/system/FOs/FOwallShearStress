wallShearStress1
{
    // Mandatory entries
    type            wallShearStress;
    libs            (fieldFunctionObjects);

    // Optional entries
    patches         (wallPatch);
    writeFields     yes;

    // Optional (inherited) entries
    writePrecision  10;
    writeToFile     true;
    useUserTime     true;

    region          region0;
    enabled         true;
    log             true;
    timeStart       0;
    timeEnd         1000;
    executeControl  timeStep;
    executeInterval 1;
    writeControl    writeTime;
    writeInterval   -1;
}


