FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "mesh";
    object      meshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

LENGTH 1.04013;
DIAMETER 0.05815;

ROUND_TIP_X 0.00804;
ROUND_TIP_R 0.00157;

NOSE_LENGTH 0.26924;
POS_AFT_CENTER_X 0.993775;
AFT_LENGTH 0.09271;

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

surfaceFile "geometry.fms";
maxCellSize 0.5;
// boundaryCellSize 0.5;
// boundaryCellSizeRefinementThickness 0.5;
// minCellSize 0.0001;

// Refine the patches within the mesh
localRefinement
{
    aft_detail
    {
        additionalRefinementLevels 11;
        refinementThickness 0.002;
    }

    body_rocket
    {
        additionalRefinementLevels 8;
        refinementThickness 0.005;
    }
}

objectRefinements
{

    refinementDomain
    {
        type        box;
        additionalRefinementLevels 1;  
        
        centre      (0.0 0.0 0.0);
        
        lengthX     100;
        lengthY     100;
        lengthZ     100;
        
    }

    refinementDomainCenter
    {
        type        box;
        additionalRefinementLevels 2;  
        
        centre      (50.0 0.0 0.0);
        
        lengthX     100;
        lengthY     #calc "$DIAMETER * 17";
        lengthZ     #calc "$DIAMETER * 17";
        
    }

    refinement0
    {
        type        box;
        additionalRefinementLevels 3;  
        
        centre      (#calc "$LENGTH / 2 + 0.2" 0.0 0.0);
        
        lengthX     #calc "$LENGTH * 3";
        lengthY     #calc "$DIAMETER * 30";
        lengthZ     #calc "$DIAMETER * 30";
        
    }

    refinement1
    {
        type        box;
        additionalRefinementLevels 5;  
        
        centre      (#calc "$LENGTH / 2 + 0.1" 0.0 0.0);
        
        lengthX     #calc "$LENGTH * 2";
        lengthY     #calc "$DIAMETER * 12";
        lengthZ     #calc "$DIAMETER * 12";
        
    }

    refinement2
    {
        type        box;
        additionalRefinementLevels 7;  
        
        centre      (#calc "$LENGTH / 2 - 0.075" 0.0 0.0);
        
        lengthX     #calc "$LENGTH * 1.15";
        lengthY     #calc "$DIAMETER * 4.5";
        lengthZ     #calc "$DIAMETER * 4.5";
        
    }

    nose_lenght1
    {
        type        box;
        additionalRefinementLevels 8;  
        
        centre      (#calc "$NOSE_LENGTH * 0.2" 0.0 0.0);
        
        lengthX     #calc "$NOSE_LENGTH * 0.7";
        lengthY     #calc "$DIAMETER";
        lengthZ     #calc "$DIAMETER";
        
    }

    nose_lenght2
    {
        type        cone;
        additionalRefinementLevels 9;  
        
        p0      (#calc "- $NOSE_LENGTH * 0.08" 0.0 0.0);
        p1      (#calc "$NOSE_LENGTH * 0.7" 0.0 0.0);
        
        radius0 #calc "$DIAMETER / 2 * 0.7";
        radius1 #calc "$DIAMETER / 2 * 1.7";

    }

    nose_lenght3
    {
        type        box;
        additionalRefinementLevels 10;  
        
        centre      (#calc "$NOSE_LENGTH * 0.05" 0.0 0.0);
        
        lengthX     #calc "$NOSE_LENGTH * 0.2";
        lengthY     #calc "$DIAMETER * 0.4";
        lengthZ     #calc "$DIAMETER * 0.4";
        
    }
    
    nose_tip1
    {
        type        sphere;
        additionalRefinementLevels 11;

        centre      (#calc "$ROUND_TIP_X * 1.4" 0.0 0.0);

        radius      #calc "$ROUND_TIP_R * 6";
    }

    nose_tip2
    {
        type        sphere;
        additionalRefinementLevels 12;

        centre      (#calc "$ROUND_TIP_X" 0.0 0.0);

        radius      #calc "$ROUND_TIP_R * 3";
    }

    nose_tip3
    {
        type        sphere;
        additionalRefinementLevels 13;

        centre      (#calc "$ROUND_TIP_X" 0.0 0.0);

        radius      #calc "$ROUND_TIP_R * 1.6";
    }


    aft_lenght1
    {
        type        cone;
        additionalRefinementLevels 10;
        
        p0      (#calc "$POS_AFT_CENTER_X + $AFT_LENGTH / 2 - 0.030" 0.0 0.0);
        p1      (#calc "$POS_AFT_CENTER_X + $AFT_LENGTH / 2 + 0.020" 0.0 0.0);
        
        radius0 #calc "$DIAMETER / 2 * 0.5";
        radius1 #calc "$DIAMETER / 2 * 1.5"; 
    }

    wake_refinement
    {

        type        cone;
        additionalRefinementLevels 7;
        
        p0      (#calc "$POS_AFT_CENTER_X + $AFT_LENGTH / 2 - 0.1" 0.0 0.0);
        p1      (#calc "$POS_AFT_CENTER_X + $AFT_LENGTH / 2 + 0.5" 0.0 0.0);
        
        radius0 #calc "$DIAMETER / 2 * 3";
        radius1 #calc "$DIAMETER / 2 * 6"; 
       
    }

    wake_refinement2
    {

        type        box;
        additionalRefinementLevels 4;
        
        centre      (#calc "$POS_AFT_CENTER_X + $AFT_LENGTH / 2 + 0.9" 0.0 0.0);;
        
        lengthX     1.8;
        lengthY     #calc "$DIAMETER * 7";
        lengthZ     #calc "$DIAMETER * 7";
       
    }

    wake_refinement3
    {

        type        box;
        additionalRefinementLevels 9;
        
        centre      (#calc "$POS_AFT_CENTER_X + $AFT_LENGTH / 2 + 0.033" 0.0 0.0);;
        
        lengthX     0.1;
        lengthY     #calc "$DIAMETER * 1.75";
        lengthZ     #calc "$DIAMETER * 1.75";
       
    }

    wake_refinement5
    {

        type        box;
        additionalRefinementLevels 8;
        
        centre      (#calc "$POS_AFT_CENTER_X + $AFT_LENGTH / 2 + 0.10" 0.0 0.0);;
        
        lengthX     0.2;
        lengthY     #calc "$DIAMETER * 1.35";
        lengthZ     #calc "$DIAMETER * 1.35";
       
    }

    wake_refinement4
    {

        type        box;
        additionalRefinementLevels 5;
        
        centre      (#calc "$POS_AFT_CENTER_X + $AFT_LENGTH / 2 + 0.7" 0.0 0.0);;
        
        lengthX     1.4;
        lengthY     #calc "$DIAMETER * 4";
        lengthZ     #calc "$DIAMETER * 4";
       
    }

    
    wake_refinement_lip
    {

        type        cone;
        additionalRefinementLevels 12;
        
        p0      (#calc "$POS_AFT_CENTER_X + $AFT_LENGTH / 2" 0.0 0.0);;
        p1      (#calc "$POS_AFT_CENTER_X + $AFT_LENGTH / 2 - 0.03" 0.0 0.0);;
        
        radius0     #calc "1.47*0.0254 / 2";
        radius1     #calc "1.47*0.0254 / 2";
       
    }
}

// Join separate patches in just one propellerTip
renameBoundary
{
	
	newPatchNames
	{
		body_rocket     {newName wall; type wall;}
        aft_detail      {newName wall; type wall;}
		
        xMin            {newName inlet; type patch;}
		xMax            {newName outlet; type patch;}

        yMin            {newName symmetry; type patch;}
		yMax            {newName far_field; type patch;}
        
        zMin            {newName far_field; type patch;}
		zMax            {newName far_field; type patch;}
	}

}

// Add inflation layers to the patches
boundaryLayers 
{
    
    thicknessRatio 1.4;
    nLayers 0;

    // allowDiscontinuity 0;
    
    patchBoundaryLayers
    {
        body_rocket
        {
            nLayers 13;
        }

        aft_detail
        {
            nLayers 13;
        }
    }
    
    optimiseLayer 1;
    // untangleLayers 0;

    optimisationParameters
    {
        nSmoothNormals 5;
        maxNumIterations 5;
        featureSizeFactor 0.3;
        reCalculateNormals 1;
        relThicknessTol 0.03;
        
    } 
            
}