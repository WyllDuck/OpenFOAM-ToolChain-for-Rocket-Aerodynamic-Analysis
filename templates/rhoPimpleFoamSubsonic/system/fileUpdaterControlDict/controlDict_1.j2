FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     rhoPimpleFoam;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         {{ minIter + 1 }};

deltaT          1;

writeControl    adjustableRunTime;

writeInterval   50;

purgeWrite      5;

writeFormat     binary;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           0.8;

rDeltaTSmoothingCoeff 0.1;

rDeltaTDampingCoeff 0.9;

maxDeltaT       1;

functions
{

    #include "FOs/FOsolverInfo"
    #include "FOs/FOforceCoeffs"

    #include "FOs/FOyPlus"
    #include "FOs/FOMach"
    #include "FOs/FOwallShearStress"

    #include "FOs/FOsurfaces"

    #include "fileUpdater"

}

